{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Automatic tasks specification",
  "description": "Automatic tasks specification",
  "type": "object",
  "properties": {
    "number_of_tasks": {
      "description": "Number of tasks",
      "type": "integer",
      "exclusiveMinimum": 0
    },
    "utilization_of_the_task_set": {
      "description": "Number of tasks",
      "type": "number",
      "exclusiveMinimum": 0
    },
    "interval_for_periods": {
      "description": "Periods intervals",
      "type": "object",
      "properties": {
        "min_period": {
          "description": "Min period",
          "type": "integer",
          "exclusiveMinimum": 0
        },
        "max_period": {
          "description": "Max period",
          "type": "integer",
          "exclusiveMinimum": 0
        }
      },
      "required": [
        "min_period",
        "max_period"
      ],
      "additionalProperties": false
    },
    "name": {
      "description": "Task generation algorithm name",
      "type": "string",
      "enum": [
        "UUniFast"
      ]
    }
  },
  "required": [
    "number_of_tasks",
    "utilization_of_the_task_set",
    "interval_for_periods",
    "name"
  ],
  "additionalProperties": false
}